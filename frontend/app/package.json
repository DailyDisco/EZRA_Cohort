{
    "name": "app",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "lint:watch": "eslint . --watch",
        "preview": "vite preview",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:run": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:watch": "vitest --watch",
        "pretty": "npx prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
        "pretty:check": "npx prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
        "format": "npm run pretty && npm run lint:fix",
        "format:check": "npm run pretty:check && npm run lint",
        "type-check": "tsc --noEmit",
        "type-check:watch": "tsc --noEmit --watch",
        "dev:full": "concurrently \"npm run dev\" \"npm run type-check:watch\" \"npm run lint:watch\"",
        "clean": "rm -rf dist coverage .vite node_modules/.vite",
        "analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html"
    },
    "dependencies": {
        "@ant-design/icons": "^5.6.1",
        "@clerk/react-router": "^1.1.3",
        "@fontsource/poppins": "^5.2.5",
        "@tanstack/react-query": "^5.66.9",
        "antd": "^5.24.2",
        "antd-style": "^3.7.1",
        "axios": "^1.8.3",
        "bootstrap": "^5.3.3",
        "react": "^19.0.0",
        "react-chatbotify": "^2.0.0-beta.32",
        "react-dom": "^19.0.0",
        "react-router": "^7.2.0",
        "react-router-dom": "^7.3.0",
        "sass": "1.77.6"
    },
    "devDependencies": {
        "@eslint/js": "^9.21.0",
        "@testing-library/jest-dom": "^6.7.0",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/react": "^19.0.10",
        "@types/react-dom": "^19.0.4",
        "@vitejs/plugin-react": "^4.3.4",
        "@vitest/coverage-v8": "^3.2.4",
        "concurrently": "^8.2.2",
        "eslint": "^9.21.0",
        "eslint-plugin-react-hooks": "^5.1.0",
        "eslint-plugin-react-refresh": "^0.4.19",
        "globals": "^15.15.0",
        "jsdom": "^26.1.0",
        "sass-embedded": "^1.85.1",
        "typescript": "~5.7.2",
        "typescript-eslint": "^8.24.1",
        "vite": "^6.2.0",
        "vite-bundle-analyzer": "^0.11.1",
        "vitest": "^3.2.4"
    }
}
